# In this language, everything is global
# namespace. Even function declarations.

@ foo () {

	# Defining a function within a function
	# will actually make it globally available
	@ bar () {
		"bar"
	}

	"foo"

	[ 0 ] {
		# But only if the code is executed
		@ bar () {
			"Not bar!"
		}
	}
}

foo()
bar()

